package library;

public class DailyExpress extends Newspaper {
	
	private String editor;


	public DailyExpress(int iD, String date, String location, String editor) {
		super(iD, date, location);
		this.editor = editor;
	}

	@Override
	public String toString() {
		return "Daily Express [ID: " + getiD() + " Date: " + getDate() + " Location: " + getLocation() + " Editor: "
				+ editor + "]";

	}

	public String getEditor() {
		return editor;
	}

	public void setEditor(String editor) {
		this.editor = editor;
	}

}
