package library;

public class TheSun extends Newspaper {

	private String editor;

	public TheSun(int iD, String date, String location, String editor) {
		super(iD, date, location);
		this.editor = editor;
	}

	@Override
	public String toString() {
		return "The Sun [ID: " + getiD() + " Date: " + getDate() + " Location: " + getLocation() + " Editor: " + editor
				+ "]";
	}

	public String getEditor() {
		return editor;
	}

	public void setEditor(String editor) {
		this.editor = editor;
	}

}
