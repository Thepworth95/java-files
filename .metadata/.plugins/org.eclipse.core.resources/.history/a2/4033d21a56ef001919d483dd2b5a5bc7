package library;

public abstract class LibraryItem {
	
	private int iD;
	private boolean checkedOut;

	
	public LibraryItem(int iD, boolean checkedOut) {
		super();
		this.iD = iD;
		this.checkedOut = checkedOut;
	}

	@Override
	public String toString() {
		return "Item [ID= " + iD + "]";
	}

	public int getiD() {
		return iD;
	}

	public void setiD(int iD) {
		this.iD = iD;
	}



}
